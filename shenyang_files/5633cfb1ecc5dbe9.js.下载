  ;'use strict';

$(function () {
  var $status = $('.status-wrapper');
  var sid = $status.attr('data-sid');
  var status = new Status($status, sid);
  status.init();

  $status.delegate('.lnk-act, .lnk-del', 'click', function (e) {
    e.preventDefault();
  }).delegate('.like', 'click', function () {
    var $this = $(this);
    $this.removeClass('like');
    $.post_withck('/j/status/like', { sid: sid }, function (r) {
      var _r = $.parseJSON(r);

      if (!_r.r) {
        $this.attr('class', 'lnk-act lnk-unlike unlike').text('已赞');
      }
    });
  }).delegate('.unlike', 'click', function () {
    var $this = $(this);
    $this.removeClass('unlike');
    $.post_withck('/j/status/unlike', { sid: sid }, function (r) {
      var _r = $.parseJSON(r);
      var likeCount = _r.count;
      if (!_r.r) {
        $this.attr('class', 'lnk-act lnk-like like').text('赞 ' + likeCount);
      }
    });
  }).delegate('.reshare:not(.new-reshare)', 'click', function () {
    var $this = $(this);
    $this.removeClass('reshare');
    $.post_withck('/j/status/reshare', { sid: sid }, function (r) {
      r = $.parseJSON(r);
      if (r.reshare_id) {
        $this.attr({
          'class': 'lnk-act lnk-unreshare unreshare',
          'data-reshare-id': r.reshare_id
        }).text('已转播');
      }
    });
  }).delegate('.lnk-del', 'click', function (e) {
    var cfm = confirm('确定要删除此条广播？');
    if (cfm) {
      $.post_withck('/j/status/delete', { sid: sid }, function (r) {
        var _r = $.parseJSON(r);
        var userUrl = $(e.target).attr('data-user_url');
        if (!_r.r) {
          window.location.href = userUrl;
        }
      });
    }
  });

  $('#comments').delegate('.admin-lnks .del-comment', 'click', function (e) {
    e.preventDefault();
    var cid = $(this).attr('data-cid');
    var cfm = confirm('确定要删除此条回应？');
    var o = $(this).parents('.comment-item');
    if (cfm) {
      $.ajax({
        url: '/j/status/comments',
        type: 'DELETE',
        data: { ck: window.get_cookie('ck'), sid: sid, cid: cid },
        success: function success() {
          o.slideUp('fast', function () {
            o.remove();
          });
        }
      });
    }
  });

  $status.delegate('.video-overlay', 'click', function (e) {
    if ($(this).hasClass('no-expand')) {
      return true;
    }
    $(this).next('.video-object').show();
    $(e.target).hide().prev().hide();
  }).delegate('.video-object .back', 'click', function (e) {
    e.preventDefault();
    $(this).parent().hide().prevAll().show();
  }).delegate('.bd blockquote a, .comments-items a:not(.btn)', 'click', function (e) {
    var el = $(e.target).parents('.status-item');
    var sid = el.attr('data-sid');
    var ac = el.attr('data-action');
    var tt = el.attr('data-target-type');
    var ok = el.attr('data-object-kind');
    moreurl(this, { from: 'nmbp-' + sid, action: ac, target_type: tt, object_kind: ok });
  });

  var $stReal = $('.status-real-wrapper:not(.say) .hd, .status-real-wrapper.say');
  $stReal.click(function (e) {
    e.stopPropagation();
    var $target = $(e.target);
    if (!$target.is('img') && !$target.is('a')) {
      e.preventDefault();
      var url = $(this).data('status-url');
      if (url) {
        window.location.href = url;
      }
    }
  });
});
